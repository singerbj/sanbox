{
  "env": {
    "browser": true
  },
  "extends": "eslint:recommended",
  "rules": {
    "semi": [
      "error",
      "always"
    ],
   "indent": ["error", 4],
   "quotes": ["error", "double"],
   "semi": ["error", "always"],
   "comma-dangle": ["error", "always"],
   "no-cond-assign": ["error", "always"],
   "no-console": "off"
  }
}
